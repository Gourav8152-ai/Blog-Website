<%- include('partials/header') %> 
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="postInput" class="form-label">Title</label>
        <input type="text" class="form-control" name="postTitle" id="postInput">
        <label for="textInput" class="form-label">Post</label>
        <textarea name="postBody" id="textInput" class="form-control" cols="30" rows="10"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Publish</button>
</form>
<%- include('partials/footer') %> 